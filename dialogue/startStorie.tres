[sub_resource type="Resource" id="Resource_mwtk7"]
script = ExtResource("1_mwtk7")
speaker = "narrator"
content = "很久很久以前，在遥远的西部荒漠。这里人迹罕见，法律崩坏。"
uiPosition = 1
metadata/_custom_type_script = "uid://rtxg2hbq8r7j"

[sub_resource type="Resource" id="Resource_vp4yp"]
script = ExtResource("1_mwtk7")
speaker = "narrator"
content = "一天两名无法之徒在这里相遇。很不幸，两人并无恩怨，但却生起矛盾。"
uiPosition = 1
metadata/_custom_type_script = "uid://rtxg2hbq8r7j"

[sub_resource type="Resource" id="Resource_dpmfy"]
script = ExtResource("1_mwtk7")
speaker = "一名无法之徒"
content = "你瞅啥？"
metadata/_custom_type_script = "uid://rtxg2hbq8r7j"

[sub_resource type="Resource" id="Resource_3pj85"]
script = ExtResource("1_mwtk7")
speaker = "另一名无法之徒"
content = "瞅你咋的？"
metadata/_custom_type_script = "uid://rtxg2hbq8r7j"

[sub_resource type="Resource" id="Resource_v0ok2"]
script = ExtResource("1_mwtk7")
speaker = "narrator"
content = "现在他们有恩怨了，于是两人决定用决斗来解决这些，然而就在这时……"
uiPosition = 1
metadata/_custom_type_script = "uid://rtxg2hbq8r7j"

[sub_resource type="Resource" id="Resource_al8i0"]
script = ExtResource("1_mwtk7")
speaker = "一名无法之徒"
content = "我说，那群呆子只知道比拔枪速度，我应该用一些不一样的方式搞定你。"
metadata/_custom_type_script = "uid://rtxg2hbq8r7j"

[sub_resource type="Resource" id="Resource_nv8gr"]
script = ExtResource("1_mwtk7")
speaker = "另一名无法之徒"
content = "你想咋样？"
metadata/_custom_type_script = "uid://rtxg2hbq8r7j"

[sub_resource type="Resource" id="Resource_iyi8r"]
script = ExtResource("1_mwtk7")
speaker = "一名无法之徒"
content = "我们先退掉所有子弹，再交换弹匣。"
metadata/_custom_type_script = "uid://rtxg2hbq8r7j"

[sub_resource type="Resource" id="Resource_nh0h2"]
script = ExtResource("1_mwtk7")
speaker = "narrator"
content = "就这样，决斗开始了……"
metadata/_custom_type_script = "uid://rtxg2hbq8r7j"

